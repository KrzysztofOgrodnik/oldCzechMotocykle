public class Dochod {
public static void main(String[] args) {
	
	Liczydlo kasa = new Liczydlo();
		
	double a = 800;
	double b = 1090;
	double c = 470;
	double d = 4.2;
	double e = 5;
	
	double addResult1 = kasa.add(a, b);
	System.out.println(addResult1);
	
	double addResult2 = kasa.subtract(addResult1, c);
	System.out.println(addResult2);
	
	double addResult3 = kasa.multiply(addResult2, d);
	System.out.println(addResult3);
	
	double addResult4 = kasa.divide(addResult3, e);
	System.out.println(addResult4);
}
}

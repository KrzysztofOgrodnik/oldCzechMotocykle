class Calculator
{
	double add(double a, double b)
	{
		double result1 = a + b;
		return result1;
	}
	double substract(double a, double b)
	{
		double result2 = a - b;
		return result2;
	}
	double multiply(double a, double b)
	{
		double result3 = a * b;
		return result3;
	}
	double divide(double a, double b)
	{
		double result4 = a / b;
		return result4;
	}
}
